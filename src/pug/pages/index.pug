	<!-- --------------------- it-offer ------------------------- -->

	section.it-offer
		.it-container
			.it-offer__wrap
				.it-offer__half.it-offer__half--left
					.it-offer__img-box
						img.it-offer__img(src="static/img/offer/1.png" title="offer")
				.it-offer__half.it-offer__half--right
					.it-offer__content
						.it-offer__title Ремонт Apple в Тюмени
						.it-offer__subtitle гарантия от 3 месяцев до 1 года
						.it-offer__price от
							span 500
							span ₽



	<!-- --------------------- it-advantages ------------------------- -->

	section.it-advantages
		.it-all-title Почему стоит обратиться в #Первый
			span.it-advantages--title Яблочный
		.it-container
			.it-advantages__wrap
				ul.it-advantages__list
					- var advantagesInfo = {"1" : "Гарантия от 3 месяцев", "2" : "Оригинальные запчасти в наличии", "3" : "Срочный ремонт от 10 минут", "4" : "Услуга выездного ремонта", "5" : "Бесплатная диагностика устройства", "6" : "Выкуп техники в любом состоянии"};
					-   for (var key in advantagesInfo) {
					.it-advantages__item
						.it-advantages__content
							.it-advantages__img-box
								img.it-price__img( src="static/img/advantages/" + key + ".png")
							.it-advantages__title #{advantagesInfo[key]}
					- }

	<!-- --------------------- it-jobs ------------------------- -->

	section.it-jobs
