        <!-- --------------------- it-offer ------------------------- -->

        section.it-offer
            .it-container
                .it-offer__wrap
                    .it-offer__half.it-offer__half--left
                        .it-offer__img-box
                            img.it-offer__img(src="static/img/offer/1.png" title="offer")
                    .it-offer__half.it-offer__half--right
                        .it-offer__content
                            .it-offer__title Ремонт Apple в Тюмени
                            .it-offer__subtitle гарантия от 3 месяцев до 1 года
                            .it-offer__price от
                                span 500
                                span ₽

